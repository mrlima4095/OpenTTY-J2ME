[ Config ]

name=ImmersiveShell
version=1.1

api.version=1.12
api.error=execute log add error ImmersiveShell requires OpenTTY 1.12;

include=forge
 
config=execute x11 stop; title; x11 item sh2me; install nano; touch; add item.label=View; add item.cmd=exec install nano & getty & add <title>$TITLE</title> & html & get nano; x11 item nano; get nano; 

item.label=Run
item.cmd=execute x11 quest sh2me;

quest.title=$TITLE
quest.label=$USERNAME $PATH $
quest.cmd=$CMD
quest.key=CMD
