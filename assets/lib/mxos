[ Config ]

name=MobiX
version=1
description=Java Mobile

api.version=1.14
api.error=execute log add error MobiX requires OpenTTY API 1.14; title MobiX Loader; warn MobiX requires OpenTTY API 1.14 or newer!; bg exec sleep 5 & exit; @stop; 

process.name=mobix

include=forge

config=execute set MobiX=1; x11 stop; x11 xfinit stdout; x11 item mxos; log add info Running MobiX $MobiX; 
command=mx

shell.name=mx
shell.args=apps

apps=execute x11 list mxos;

[ COMMAND ]

item.label=MobiX
item.cmd=

[ DISPLAY ]

screen.title=
screen.content=
screen.content.style=
screen.back.label=
screen.back=
screen.button=
screen.button.cmd=

quest.title=$TITLE
quest.label=$LABEL
quest.key=$INPUT
quest.cmd=execute $CMD $INPUT; unset CMD; unset LABEL; unset INPUT;
quest.back=execute $BACK; unset BACK; unset CMD; unset LABEL; 

list.title=
list.content=
list.back.label=
list.back=
list.button=
