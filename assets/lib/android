[ Config ]

name=Android
version=1.0
description=API for Android

api.version=1.16
api.error=execute log add error Android API requires OpenTTY 1.16; echo [ Android ] Required 1.16 OpenTTY API;
api.match=minimum

process.name=android
process.exit=execute kill 10542; stop android;
process.port=10542
process.db=droid

include=/java/bin/yang,/java/bin/jauth2,forge

config=execute set DROID_VERSION=1.0; log add info Running Android API $DROID_VERSION; echo [ Android ] Running Android API $DROID_VERSION; x11 item android;
command=droid

droid=droid menu

shell.name=droid
shell.args=help,menu

help=open https://github.com/mrlima4095/OpenTTY-J2ME/blob/main/assets/root/android.md
menu=execute x11 list android;


[ COMMAND ]

item.label=Android
item.cmd=execute droid menu

[ DISPLAY ]

list.title=Android API
list.content=Yang Repository,Stop Android API,Online README
list.button=Select


Yang Repository=execute yang;


Stop Android API=execute droid stop
Online README=execute droid help
 
