[ Config ]

name=Forge
version=1.2
description=Mod Editor

config=execute set FORGE_VERSION=1.2; 
command=forge

forge=execute x11 make forge; 

[ DISPLAY ]

screen.title=Forge $FORGE_VERSION
screen.button=Tools
screen.button.cmd=execute x11 list forge;

list.title=Forge Tools
list.content=New Project
list.back=execute x11 make forge;

New Project=execute touch; add [ Config ]\n\nname=App Name\nversion=1.0\ndescription=My Project\n\ninclude=forge\n\nprocess.name=app\nmod=\n\nconfig=execute true\ncommand=\n; nano; 

