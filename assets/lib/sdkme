[ Config ]

name=SmartME
version=1.1
description=OpenTTY Toolkit

api.version=1.16
api.error=execute 

include=forge

config=execute set SDK=1.1;
command=sdk

shell.name=sdk
shell.args=new

new=execute touch; add #include "OpenTTY.h"; add; add int main() {; add; add }; nano; 



[ DISPLAY ]

list.title=SmartME SDK
list.content=New Project,Build (from nano),Download Libraries,Update SDK,About

New Project=execute sdk new;
Build (from nano)=execute build nano;
Download Libraries=exec case alias (yang) execute set REQUEST=OpenTTY.h; yang install; set REQUEST=X11.h; yang install;
Documentation=execute view <title>SmartME Docs</title>;
Update SDK=execute install nano; tick Downloading...; proxy raw.githubusercontent.com/mrlima4095/OpenTTY-J2ME/main/assets/lib/sdkme; install sdkme; get nano; import sdkme; tick; forge; warn SmartME was installed the latest version from GitHub;
About=execute warn SmartME $SDK OpenTTY Toolkit;