[ Config ]

name=JBenchmark
version=1.2
description=Debuggers 

api.version=1.11
api.error=execute echo [JBenchmark] This version requires OpenTTY 1.11;

command=dumpsys

dumpsys=execute dumpsys run; true 


shell.name=dumpsys
shell.args=run,sys,net

sys=execute pkg /microedition.configuration; add MicroEdition Config: $OUTPUT; pkg /microedition.profiles; add MicroEdition Profiles: $OUTPUT; pkg /microedition.platform; add MicroEdition Platform: $OUTPUT; pkg /microedition.encoding; add MicroEdition Encoding: $OUTPUT; add;
net=execute netstat; if ($OUTPUT == true) exec ifconfig & add IP Adress: $OUTPUT & fw ip & add Global Addr: $OUTPUT; if ($OUTPUT == false) add Not Connected with Internet; add; 


run=execute start dumpsys; tick Debugging...; install nano; getty; install stdout-content-backup; clear; touch; add JBenchmark Result; add; dumpsys sys; # dumpsys net; clear; raw; set LOGS=$OUTPUT; tick; x11 make debuggers; get stdout-content-backup; rm stdout-content-backup; rraw; get nano; stop dumpsys;


[ DISPLAY ]

screen.title=JBenchmark
screen.content=$LOGS
screen.back=execute unset LOGS;
screen.button=Menu
screen.button.cmd=execute x11 list debuggers;

quest.title=JBenchmark
quest.label=Save Logs (filename)
quest.key=FILENAME
quest.cmd=execute x11 list debuggers; install nano; touch; add $LOGS; install $FILENAME; get nano; unset FILENAME;
quest.back=execute x11 list debuggers;

list.title=JBenchmark
list.content=Save Logs,Run Again
list.back=execute x11 make debuggers;

Save Logs=execute x11 quest debuggers;
Run Again=execute dumpsys;
