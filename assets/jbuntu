#!/java/bin/java
# -*- coding: utf-8 -*-
#    
#    run jbuntu
#    
#  Copyright (C) 2024 "Mr. Lima"
#  


name=JBuntu
version=1.0
description=Desktop for OpenTTY J2ME

config=execute set JVERSION=1.0; title JBuntu $JVERSION; clear;
command=xterm

xterm=execute x11 make jbuntu; 


screen.title=JBuntu $JVERSION 
screen.back=exit
screen.button=Menu
screen.button.cmd=x11 list jbuntu


list.title=Menu
list.content=About,Nano,Explorer,Terminal

About=execute clear; buff xterm; about jbuntu;
Nano=execute clear; buff xterm; nano;
Explorer=execute clear; buff xterm; dir f;
Terminal=execute buff; sh;

