[ Config ]

name=Bruteforce
version=1.0

[ DISPLAY ]

quest.title=Bruteforce
quest.label=Command
quest.key=CMD
quest.cmd=execute bruteforce -c $CMD; unset CMD;
quest.back=execute x11 list /java/etc/bruteforce;

list.title=BruteForce
list.content=Run Command
list.button=Open

Run Command=execute x11 quest /java/etc/bruteforce;
Run Command-INFO=Execute commands in loop
