[ Config ]

name=PackJ
version=1.5
description=OpenTTY Package Manager

include=/lib/settings

config=case !key (REPO) set REPO=opentty.xyz:31522
command=yang,setrepo

yang=execute x11 list /lib/yang;
setrepo=execute set VALUE=REPO; set LABEL=IP Address (OpenTTY Server); export RETURN; cfg run;

shell.name=yang
shell.args=install,update,setrepo,info

install=sudo bg execute case !user (root) exec echo ":: Root permission required!" & return 13; case file (/tmp/.yang-lock) exec echo ":: Another yang instance is running!" & return 68; set OLD_QUERY=$QUERY; set QUERY=$__LOCAL; echo ":: Downloading $RESOURCE"; query socket://$REPO get lib/$RESOURCE; echo ":: Installed [ $__LOCAL ]"; set QUERY=$OLD_QUERY; unset OLD_QUERY; unset RESOURCE; unset __LOCAL; 
update=echo :: Still in development

info=execute echo "PackJ 1.5 (Default)"

[ Index ]

list.title=Repository
list.content=Android ME,Armitage,Auto Clean,Back Previous,BoxME,CMatrix,Discord (MIDlet),Forge,Github (MIDlet),GoBuster (Word list),ImmersiveShell,JBuntu,JBenchmark,J2ME Loader,MobiX UI,PasteBin,SmartME SDK,Updater,ViaVersion
list.button=Install

Android ME=execute set RESOURCE=android; set __LOCAL=/bin/droid; yang install; set RESOURCE=forge; set LOCAL=/bin/forge; yang install;
Armitage=execute set RESOURCE=armitage; set __LOCAL=/bin/armitage; yang install;
Auto Clean=execute set RESOURCE=autogc; set __LOCAL=/lib/autogc; yang install;
Back Previous=execute set RESOURCE=bprevious; set __LOCAL=/lib/bprevious; yang install;
BoxME=execute set RESOURCE=boxme; set __LOCAL=/bin/boxme; yang install;
CMatrix=execute set RESOURCE=cmatrix; set __LOCAL=/bin/cmatrix; yang install;
DarkDM+=execute set RESOURCE=darkdm.lua; set __LOCAL=/bin/ddm; yang install
Discord (MIDlet)=execute warn "This is a 3rd MIDlet from 'gtrxac'"; bg exec sleep 3 & open http://146.59.80.3/discord_midp2_beta.jar;
Forge=execute set RESOURCE=forge; set __LOCAL=/bin/forge yang install;
Github (MIDlet)=execute warn "This is a 3rd MIDlet from 'shinovon'"; bg exec sleep 3 & open http://nnp.nnchan.ru/dl/GH2ME.jar;
GoBuster (Word list)=execute set RESOURCE=gobuster; set __LOCAL=/home/gobuster; yang install;
ImmersiveShell=execute set RESOURCE=sh2me; set __LOCAL=/lib/sh2me; yang install;
JBuntu=execute set RESOURCE=jbuntu; set __LOCAL=/bin/jbuntu; yang install;
JBenchmark=execute set RESOURCE=debuggers; set __LOCAL=/bin/jdb; yang install;
J2ME Loader=execute set RESOURCE=modme; set __LOCAL=/bin/modme; yang install;
MobiX UI=execute set RESOURCE=mxos; set __LOCAL=/bin/mxos; yang install;
PasteBin=execute set RESOURCE=pastebin; set __LOCAL=/bin/pastebin; yang install;
SmartME SDK=execute set RESOURCE=sdkme; set __LOCAL=/bin/sdk; yang install; set RESOURCE=forge; set LOCAL=/bin/forge; yang install;
Updater=execute set RESOURCE=sync; set __LOCAL=/bin/sync; yang install;
ViaVersion=execute set RESOURCE=viaversion; set __LOCAL=/lib/viaversion; yang install;
WebProxy=execute set RESOURCE=proxy.lua; set __LOCAL=/bin/shprxy; yang install
