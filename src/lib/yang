[ Config ]

name=PackJ
version=1.5
description=OpenTTY Package Manager

include=/lib/settings

config=case !key (REPO) set REPO=opentty.xyz:31522
command=yang,setrepo

yang=execute x11 list /lib/yang;
setrepo=execute set VALUE=REPO; set LABEL=IP Address (OpenTTY Server); export RETURN; cfg run;

shell.name=yang
shell.args=install,update,setrepo,info

install=sudo bg execute case file (/tmp/.yang-lock) exec echo ":: Another yang instance is running!" & return 68; set OLD_QUERY=$QUERY; set QUERY=$PKJOUT; echo ":: Downloading $RESOURCE"; query socket://$REPO get lib/$RESOURCE; echo ":: Installed [ $PKJOUT ]"; set QUERY=$OLD_QUERY; unset OLD_QUERY RESOURCE PKJOUT; 
update=echo :: Still in development

info=execute echo "PackJ 1.5 (Default)"

[ Index ]

list.title=Repository
list.content=Android ME,Armitage,Auto Clean,Back Previous,BoxME,CMatrix,Discord (MIDlet),Forge,Github (MIDlet),GoBuster (Word list),ImmersiveShell,JBuntu,JBenchmark,J2ME Loader,MobiX UI,PasteBin,SmartME SDK,Updater,ViaVersion,WebProxy
list.button=Install

Android ME=execute set RESOURCE=android; set PKJOUT=/bin/droid; yang install; set RESOURCE=forge; set LOCAL=/bin/forge; yang install;
Armitage=execute set RESOURCE=armitage; set PKJOUT=/bin/armitage; yang install;
Auto Clean=execute set RESOURCE=autogc; set PKJOUT=/lib/autogc; yang install;
Back Previous=execute set RESOURCE=bprevious; set PKJOUT=/lib/bprevious; yang install;
BoxME=execute set RESOURCE=boxme; set PKJOUT=/bin/boxme; yang install;
CMatrix=execute set RESOURCE=cmatrix; set PKJOUT=/bin/cmatrix; yang install;
DarkDM+=execute set RESOURCE=darkdm.lua; set PKJOUT=/bin/ddm; yang install
Discord (MIDlet)=execute warn "This is a 3rd MIDlet from 'gtrxac'"; bg exec sleep 3 & open http://146.59.80.3/discord_midp2_beta.jar;
Forge=execute set RESOURCE=forge; set PKJOUT=/bin/forge; yang install;
Github (MIDlet)=execute warn "This is a 3rd MIDlet from 'shinovon'"; bg exec sleep 3 & open http://nnp.nnchan.ru/dl/GH2ME.jar;
GoBuster (Word list)=execute set RESOURCE=gobuster; set PKJOUT=/home/gobuster; yang install;
ImmersiveShell=execute set RESOURCE=sh2me; set PKJOUT=/lib/sh2me; yang install;
JBuntu=execute set RESOURCE=jbuntu; set PKJOUT=/bin/jbuntu; yang install;
JBenchmark=execute set RESOURCE=debuggers; set PKJOUT=/bin/jdb; yang install;
J2ME Loader=execute set RESOURCE=modme; set PKJOUT=/bin/modme; yang install;
MobiX UI=execute set RESOURCE=mxos; set PKJOUT=/bin/mxos; yang install;
PasteBin=execute set RESOURCE=pastebin; set PKJOUT=/bin/pastebin; yang install;
SmartME SDK=execute set RESOURCE=sdkme; set PKJOUT=/bin/sdk; yang install; set RESOURCE=forge; set LOCAL=/bin/forge; yang install;
Updater=execute set RESOURCE=sync; set PKJOUT=/bin/sync; yang install;
ViaVersion=execute set RESOURCE=viaversion; set PKJOUT=/lib/viaversion; yang install;
WebProxy=execute set RESOURCE=proxy.lua; set PKJOUT=/bin/shprxy; yang install
