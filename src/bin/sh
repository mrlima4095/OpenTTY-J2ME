#!/bin/lua

for k,v in pairs({ 
    ["PATCH"] = "Fear Fog", 
    ["VERSION"] = getAppProperty("MIDlet-Version"), 
    ["RELEASE"] = "stable", 
    ["HOSTNAME"] = "localhost",
    ["SHELL"] = "/bin/sh"
}) do
    print(k, v)
end

for k,v in pairs({ 
    ["TYPE"] = "platform", 
    ["CONFIG"] = "configuration", 
    ["PROFILE"] = "profiles", 
    ["LOCALE"] = "locale" 
}) do
    print(getAppProperty("/microedition." .. v))
end

