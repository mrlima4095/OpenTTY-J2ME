#!/bin/lua

local function shell(args)
    local argument = {}
    for i = 2,#args then
        argument[i] = args[i]
    end
    argument = table.concat(argument)
    
    if args[1] == "" then
    elseif io.open("/bin/" .. args[1]) then
        local res = io.popen("/bin/" .. args[1], table.concat(), false, io.stdout)
        return res.status
    elseif args[1] == "env" then
    elseif args[1] == "echo" then
        print(argument)
    elseif args[1] == "clear" then
        io.write("", io.stdout)
    else
        print(args[1] .. ": not found")
        return 127
    end

    return 0
end

os.exit(shell(arg))