#!/bin/lua

-- Configurar vari√°veis de ambiente
for k,v in pairs({ 
    ["PATCH"] = "Fear Fog", 
    ["VERSION"] = getAppProperty("MIDlet-Version"), 
    ["RELEASE"] = "stable", 
    ["HOSTNAME"] = "localhost", 
    ["SHELL"] = "/bin/sh" 
}) do
    os.setenv(k, v)
end

for k,v in pairs({ 
    ["TYPE"] = "platform", 
    ["CONFIG"] = "configuration", 
    ["PROFILE"] = "profiles", 
    ["LOCALE"] = "locale" 
}) do
    os.setenv(k, getAppProperty("/microedition." .. v))
end



os.execute("env")